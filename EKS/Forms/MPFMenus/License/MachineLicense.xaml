        <Button x:Name="CancelBTN" Content="İptal" HorizontalAlignment="Left" Margin="201,64,0,0" VerticalAlignment="Top" Width="75" Click="CancelBTN_Click"/>
        <Button x:Name="LicenseAddBTN" Content="Lisans Ekle" HorizontalAlignment="Left" Margin="121,64,0,0" VerticalAlignment="Top" Width="75" Click="LicenseAddBTN_Click"/>
