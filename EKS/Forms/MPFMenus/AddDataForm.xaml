        <ComboBox x:Name="ComputerModelCMBBX" HorizontalAlignment="Left" Margin="190,320,0,0" VerticalAlignment="Top" Width="222" Height="26"/>
        <ComboBox x:Name="OperatorSystemCMBBX" HorizontalAlignment="Left" Margin="190,350,0,0" VerticalAlignment="Top" Width="222" Height="26"/>
